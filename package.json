{
    "name": "@datavysta/vysta-cli",
    "version": "1.0.3",
    "description": "Generate TypeScript models from your Vysta server",
    "main": "dist/index.js",
    "type": "module",
    "bin": {
        "vysta-cli": "./dist/index.js"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc",
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
        "prepublishOnly": "npm run build",
        "test:generate": "node ./dist/index.js http://localhost:8080 --output ./test-output",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/datavysta/vysta-cli.git"
    },
    "keywords": [
        "vysta",
        "datavysta",
        "api",
        "client",
        "typescript",
        "rest"
    ],
    "author": "DataVysta <dev@datavysta.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/datavysta/vysta-cli/issues"
    },
    "homepage": "https://www.datavysta.com/",
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "commander": "^11.1.0",
        "inquirer": "^9.2.12",
        "ts-morph": "^21.0.1"
    },
    "devDependencies": {
        "@types/inquirer": "^9.0.7",
        "@types/jest": "^29.5.11",
        "@types/node": "^20.10.5",
        "@typescript-eslint/eslint-plugin": "^6.15.0",
        "@typescript-eslint/parser": "^6.15.0",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.0.1",
        "jest": "^29.7.0",
        "prettier": "^3.1.1",
        "ts-jest": "^29.1.1",
        "typescript": "^5.3.3"
    }
}
